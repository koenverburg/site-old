{
  "name": "site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "install:all": "pnpm recursive install",
    "build:apps": "pnpm recursive run build --filter @apps/*",
    "serve:docs": "pnpm run serve --filter @apps/docs",
    "serve:web": "pnpm run serve --filter @apps/web",
    "build": "pnpm recursive run build",
    "lint": "eslint -c packages/config/eslint-preset.js",
    "lint:old": "pnpm recursive run lint",
    "test": "pnpm recursive run test",
    "typecheck": "pnpm recursive run typecheck",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "lint:fix": "pnpm recursive run lint:fix"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.15.4"
  },
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@next/react-dev-overlay": "^12.0.8",
    "@typescript-eslint/parser": "^5.9.1",
    "esbuild": "^0.14.8",
    "esbuild-jest": "^0.5.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.4.5",
    "next": "12.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-jsx": "^4.0.1",
    "typescript": "4.5.2"
  }
}
